    {
      "id": "swing_chain",
      "type": "line",
      "source": "swing_chain",
      "minzoom": 12,
      "paint": {"line-color": "#474747", "line-width": 2, "line-opacity": 0.9}
    },
    {
      "id": "vessel_symbol",
      "type": "symbol",
      "source": "vessel_point",
      "minzoom": 7,
      "maxzoom": 12,
      "filter": [
        "all",
        ["<", ["get", "course"], 360],
        [
          "!",
          [
            "in",
            ["get", "vesselTypeId"],
            ["literal", [0, 34, 36, 37, 50, 51, 52, 180]]
          ]
        ]
      ],
      "layout": {
        "icon-size": 0.35,
        "icon-allow-overlap": true,
        "icon-rotate": ["get", "course"],
        "visibility": "visible",
        "icon-image": [
          "match",
          ["get", "statusPort"],
          "ANCHORED",
          "anchoring",
          "BANNED",
          "banned",
          "BERTH",
          "berth",
          "BUNKER_OPS",
          "bunker_ops",
          "CARGO_OPS",
          "cargo_ops",
          "DETAINED",
          "detained",
          "DRIFTING_OPS",
          "drifting_ops",
          "OUT_OF_RANGE",
          "out_of_range",
          "PILOT_ON_BOARD",
          "pilot_on_board",
          "READY_TO_SAIL",
          "ready_to_sail",
          "READY_TO_SAIL_AUTORIZED",
          "ready_to_sail_autorized",
          "READY_TO_SAIL_NO_AUTORIZED",
          "ready_to_sail_no_autorized",
          "TERMINAL_READY",
          "terminal_ready",
          "TERMINAL_READY_AUTORIZED",
          "terminal_ready_autorized",
          "TERMINAL_READY_NO_AUTORIZED",
          "terminal_ready_no_autorized",
          "TRANSIT",
          "transit",
          "WATER_OPS",
          "water_ops",
          "transit"
        ]
      }
    },
    {
      "id": "vessel_val",
      "type": "symbol",
      "source": "vessel_val",
      "minzoom": 7,
      "maxzoom": 12,
      "layout": {
        "icon-size": 0.35,
        "icon-allow-overlap": true,
        "icon-rotate": ["get", "course"],
        "visibility": "visible",
        "icon-image": "cargo_ops"
      }
    },
    {
      "id": "vessel_pol",
      "type": "fill",
      "source": "vessel_pol",
      "minzoom": 12,
      "filter": [
        "all",
        ["<", ["get", "heading"], 360],
        ["!=", ["get", "vesselTypeId"], 0],
        ["!=", ["get", "vesselTypeId"], 36],
        ["!=", ["get", "vesselTypeId"], 37]
      ],
      "paint": {
        "fill-color": [
          "match",
          ["get", "statusPort"],
          "BERTH",
          "#f84b66",
          "ANCHORED",
          "#c8d1ec",
          "BANNED",
          "#696f70",
          "BUNKER_OPS",
          "#d08d37",
          "CARGO_OPS",
          "#ad0505",
          "DETAINED",
          "#696f70",
          "DRIFTING_OPS",
          "#d0ff33",
          "OUT_OF_RANGE",
          "#d2eed6",
          "PILOT_ON_BOARD",
          "#f1ef79",
          "READY_TO_SAIL",
          "#d08787",
          "TERMINAL_READY",
          "#ffd3d3",
          "TRANSIT",
          "#6db965",
          "WATER_OPS",
          "#01ced0",
          "#ffffff"
        ],
        "fill-opacity": ["match", ["get", "statusPort"], "BERTH", 0.4, 1],
        "fill-outline-color": "transparent"
      }
    },
    {
      "id": "stroke_vessel_others",
      "type": "line",
      "source": "vessel_pol",
      "minzoom": 12,
      "filter": [
        "all",
        ["!=", ["get", "statusPort"], "BERTH"],
        ["<", ["get", "heading"], 360],
        ["!=", ["get", "vesselTypeId"], 0],
        ["!=", ["get", "vesselTypeId"], 36],
        ["!=", ["get", "vesselTypeId"], 37]
      ],
      "layout": {"visibility": "visible"},
      "paint": {"line-color": "#4482a5", "line-width": 1.2}
    },
    {
      "id": "stroke_vessel_berth",
      "type": "line",
      "source": "vessel_pol",
      "minzoom": 12,
      "filter": [
        "all",
        ["==", ["get", "statusPort"], "BERTH"],
        ["<", ["get", "heading"], 360],
        ["!=", ["get", "vesselTypeId"], 0],
        ["!=", ["get", "vesselTypeId"], 36],
        ["!=", ["get", "vesselTypeId"], 37]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "#4482a5",
        "line-width": 1.2,
        "line-dasharray": [3, 3]
