    {
      "id": "vessel_enhanced",
      "type": "fill",
      "source": "vessel_enhanced",
      "minzoom": 12,
      "paint": {
        "fill-color": "#f84b66",
        "fill-opacity": 1,
        "fill-outline-color": "#4482a5"
      }
    },
    {
      "id": "vessel_enhanced_line",
      "type": "line",
      "source": "vessel_enhanced",
      "minzoom": 12,
      "paint": {"line-color": "#4482a5", "line-width": 1.4, "line-opacity": 1}
    },
    {
      "id": "swing_circle",
      "type": "fill",
      "source": "swing_circle",
      "minzoom": 12,
      "paint": {"fill-color": "#808080", "fill-opacity": 0.2}
    },
    {
      "id": "swing_circle_line",
      "type": "line",
      "source": "swing_circle",
      "minzoom": 12,
